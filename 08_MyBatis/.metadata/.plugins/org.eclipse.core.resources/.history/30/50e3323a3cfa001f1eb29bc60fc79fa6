<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="memberMapper">
  <select id="loginMember" resultType="Member">
    select USER_NO,
			USER_ID,
			USER_PWD,
			USER_NAME,
			EMAIL,
			BIRTHDAY,
			GENDER,
			PHONE,
			ADDRESS,
			ENROLL_DATE,
			MODIFY_DATE,
			STATUS
	  FROM MEMBER
	  WHERE USER_ID = ?
	  AND USER_PWD = ?
	  AND STATUS = 'Y'
  </select>
</mapper>