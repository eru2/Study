<mapper namespace="memberMapper">
<resultMap type="Member">
<!-- <result column="테이블의 컬럼명" property="객체의 필드명" />  -->
<result column="USER_NO" property="userNo" />
<result column="USER_ID" property="userNo" />
<result column="USER_PWD" property="userNo" />
<result column="USER_NAME" property="userNo" />
<result column="EMAIL" property="userNo" />
<result column="BIRTHDAY" property="userNo" />
<result column="GENDER" property="userNo" />
<result column="PHONE" property="userNo" />
<result column="ADDRESS" property="userNo" />
<result column="ENROLL_DATE" property="userNo" />
<result column="MODIFY_DATE" property="userNo" />
<result column="STATUS" property="userNo" />

</resultMap>
  <select id="loginMember" parameterType="Member" resultMap="">
    select USER_NO,
			USER_ID,
			USER_PWD,
			USER_NAME,
			EMAIL,
			BIRTHDAY,
			GENDER,
			PHONE,
			ADDRESS,
			ENROLL_DATE,
			MODIFY_DATE,
			STATUS
	  FROM MEMBER
	  WHERE USER_ID = #{userId} <!-- getuserId -->
	  AND USER_PWD = #{userPwd}
	  AND STATUS = 'Y'
  </select>
</mapper>